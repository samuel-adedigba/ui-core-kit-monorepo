{
  "name": "ui-core-kit",
  "type": "module",
  "version": "0.0.2",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "style": "dist/index.css",
  "publishConfig": {
    "access": "public"
  },
  "description": "A reusable UI core component kit built with React, Tailwind, and Framer Motion.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuel-adedigba/ui-core-kit-monorepo"
  },
  "bugs": {
    "url": "https://github.com/samuel-adedigba/ui-core-kit-monorepo/issues"
  },
  "homepage": "https://github.com/samuel-adedigba/ui-core-kit-monorepo#readme",
  "author": "Your Name samueladedigba83@gmail.com",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./theme": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "build:css": "tailwindcss -c tailwind.config.cjs -i ./src/index.css -o ./dist/tailwind.css --minify",
    "dev": "rollup -c -w",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier src --write",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.esm.js",
      "ignore": [
        "fs",
        "path",
        "os",
        "util",
        "module",
        "crypto"
      ],
      "limit": "500 KB"
    }
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.3.1"
  },
  "dependencies": {
    "@fullcalendar/core": "~6.1.18",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/react": "^6.1.8",
    "@fullcalendar/timegrid": "^6.1.19",
    "@popperjs/core": "^2.11.8",
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/match-sorter-utils": "^8.8.4",
    "@tanstack/react-table": "^8.8.5",
    "@visx/pattern": "^3.0.0",
    "apexcharts": "^3.37.3",
    "axios": "^1.3.4",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "framer-motion": "^12.23.12",
    "history": "^5.3.0",
    "html-react-parser": "^3.0.15",
    "i18next": "^22.4.13",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react-apexcharts": "^1.7.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-highlight-words": "^0.20.0",
    "react-i18next": "^12.2.0",
    "react-icons": "^4.8.0",
    "react-markdown": "^8.0.7",
    "react-modal": "^3.16.1",
    "react-number-format": "^5.1.4",
    "react-popper": "^2.3.0",
    "react-portal": "^4.2.2",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.9.0",
    "react-scroll": "^1.8.9",
    "react-select": "^5.7.2",
    "react-simple-maps": "^3.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.11.1",
    "twin.macro": "^3.3.0",
    "yup": "^1.1.1"
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "config": "./tailwind.config.cjs"
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@size-limit/preset-small-lib": "^11.2.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.15.5",
    "@types/react": "^18.0.28",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.0.11",
    "@types/react-highlight-words": "^0.16.4",
    "@types/react-modal": "^3.13.1",
    "@types/react-portal": "^4.0.4",
    "@types/react-scroll": "^1.8.7",
    "@types/react-simple-maps": "^3.0.0",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "miragejs": "^0.1.47",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-nesting": "^11.2.1",
    "prettier": "^2.8.6",
    "react-redux": "^8.0.5",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "rollup": "^4.46.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^11.2.0",
    "typescript": "^4.9.5",
    "vite": "^4.3.3",
    "vite-plugin-dynamic-import": "^1.3.4"
  },
  "keywords": [
    "react",
    "ui",
    "components",
    "theme",
    "design-system"
  ],
  "license": "MIT"
}
